<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <AssemblyName>JinnDev.IOC</AssemblyName>
    <RootNamespace>JinnDev.IOC</RootNamespace>
    <Version>1.0.0</Version>
    <Authors>JinnDev</Authors>
    <Company>Jinn Studios</Company>
    <Product>IOC</Product>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <PackageOutputPath>..\..\..\..\NuGetSource</PackageOutputPath>
    <RestoreSources>$(RestoreSources);..\..\..\..\NuGetSource;https://api.nuget.org/v3/index.json</RestoreSources>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JinnDev.IOC.Core" Version="1.0.0" />
    <PackageReference Include="JinnDev.IOC.Data.Core" Version="1.0.0" />
    <PackageReference Include="JinnDev.SingularIOC.SDK" Version="1.0.0" />
  </ItemGroup>

</Project>